{
	"name": "VS-code-drupal",
	"displayName": "Drupal Syntax Highlighting",
	"description": "Provides syntax highlighting support for Drupal specific file types, such as .module, .inc, .theme etc.",
	"version": "0.0.12",
	"engines": {
		"vscode": "1.x.x"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/mastazi/VS-code-drupal"
	},
	"license": "SEE LICENSE IN LICENSE.txt",
	"icon": "images/icon.svg",
	"categories": [
		"Other"
	],
	"publisher": "marcostazi",
	"extensionDependencies": [
		"php"
	],
	"contributes": {
		"languages": [
			{
				"id": "php",
				"aliases": [
					"PHP",
					"php"
				],
				"extensions": [
					".engine",
					".theme",
					".install",
					".inc",
					".make",
					".module",
					".profile",
					".test"
				]
			},
			{
				"id": "ini",
				"aliases": [
					"Ini",
					"ini"
				],
				"extensions": [
					".info"
				]
			}
		]
	},
